    "normalize-exception": "^2.11.0",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-markdown": "^8.0.7",
    "react-material-symbols": "^4.1.1",
    "react-router-dom": "^6.15.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-timeago": "^7.1.0",
    "react-transition-group": "^4.4.5",
    "remark-breaks": "^3.0.3",
    "remark-gfm": "^3.0.1",
    "shallow-equal": "^3.1.0",
    "use-debounce": "^9.0.4",
    "uuid": "^9.0.0"
  }
