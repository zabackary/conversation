{
  "name": "conversation",
  "version": "4.0.0-dev",
  "description": "A React-powered chatting app for Google Scripts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env VITE_ONLYSERVER=1 vite build && cross-env VITE_ONLYCLIENT=1 VITE_SECONDPASS=1 vite build",
    "build:server-dev": "cross-env NODE_ENV=production VITE_ONLYSERVER=1 VITE_UPLOADONCOMPLETE=1 VITE_SECONDPASS=1 vite build --watch",
    "upload": "clasp push",
    "deploy": "pnpm run build && pnpm run upload",
    "serve": "cross-env NODE_ENV=local vite serve",
    "lint": "eslint"
  },
  "devDependencies": {
