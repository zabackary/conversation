{
  "name": "conversation",
  "version": "v4.0.0-alpha.2",
  "description": "A React-powered chatting app for Google Scripts",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "cross-env VITE_ONLYSERVER=1 vite build && cross-env VITE_ONLYCLIENT=1 VITE_SECONDPASS=1 vite build",
    "build:server-dev": "cross-env NODE_ENV=production VITE_ONLYSERVER=1 VITE_UPLOADONCOMPLETE=1 VITE_SECONDPASS=1 vite build --watch",
    "upload": "clasp push",
    "deploy": "pnpm run build && pnpm run upload",
    "serve": "cross-env NODE_ENV=local vite serve --host",
    "lint": "eslint",
    "pull-types": "supabase gen types typescript --local > src/@types/supabase.d.ts && prettier src/@types/supabase.d.ts --write"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/generator": "^7.22.10",
    "@babel/parser": "^7.22.10",
    "@babel/template": "^7.22.5",
    "@babel/traverse": "^7.24.7",
    "@babel/types": "^7.22.10",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@google/clasp": "^2.4.2",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__template": "^7.4.1",
    "@types/babel__traverse": "^7.20.6",
    "@types/canvas-confetti": "^1.6.0",
    "@types/eslint": "^8.44.2",
    "@types/google-apps-script": "^1.0.67",
    "@types/google-apps-script-oauth2": "^38.0.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@types/react-timeago": "^4.1.3",
    "@types/react-transition-group": "^4.4.6",
    "@types/uuid": "^9.0.2",
    "@types/which": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "match-sorter": "^6.3.1",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.9.2",
    "typescript": "^5.4.5",
    "vite": "^4.5.3",
    "vite-plugin-checker": "^0.6.4",
    "which": "^3.0.1"
  },
  "dependencies": {
    "@axe-core/react": "^4.7.3",
    "@babel/runtime": "^7.22.10",
    "@emoji-mart/react": "^1.1.1",
    "@material/material-color-utilities": "^0.2.7",
    "@mui/material": "^5.14.5",
    "@mui/utils": "^5.14.5",
    "@supabase/supabase-js": "^2.47.3",
    "@types/bcryptjs": "^2.4.2",
    "bcryptjs": "^2.4.3",
    "canvas-confetti": "^1.6.0",
